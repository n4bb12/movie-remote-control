{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "defaultProject": "movie-remote-control",
  "newProjectRoot": "projects",
  "projects": {
    "movie-remote-control": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "production": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "vendorChunk": false
            }
          },
          "options": {
            "assets": ["src/favicon.ico", "src/assets", "src/robots.txt"],
            "index": "src/index.html",
            "main": "src/main.ts",
            "outputPath": "dist/movie-remote-control",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "styles": ["src/styles.scss"],
            "tsConfig": "src/tsconfig.app.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "movie-remote-control:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "configurations": {
            "production": {
              "fix": false
            }
          },
          "options": {
            "exclude": ["**/node_modules/**"],
            "fix": true,
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "movie-remote-control:build:production"
            }
          },
          "options": {
            "browserTarget": "movie-remote-control:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "configurations": {
            "production": {
              "browsers": "ChromeHeadlessCI",
              "codeCoverage": true,
              "progress": false,
              "watch": false
            }
          },
          "options": {
            "assets": ["src/favicon.ico", "src/assets", "src/robots.txt"],
            "karmaConfig": "src/karma.conf.js",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "styles": ["src/styles.scss"],
            "tsConfig": "src/tsconfig.spec.json"
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "sourceRoot": "src"
    },
    "movie-remote-control-e2e": {
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "configurations": {
            "production": {
              "devServerTarget": "movie-remote-control:serve:production",
              "protractorConfig": "./e2e/protractor-ci.conf.js"
            }
          },
          "options": {
            "devServerTarget": "movie-remote-control:serve",
            "protractorConfig": "e2e/protractor.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "configurations": {
            "production": {
              "fix": false
            }
          },
          "options": {
            "exclude": ["**/node_modules/**"],
            "fix": true,
            "tsConfig": "e2e/tsconfig.e2e.json"
          }
        }
      },
      "prefix": "",
      "projectType": "application",
      "root": "e2e/"
    }
  },
  "version": 1
}
